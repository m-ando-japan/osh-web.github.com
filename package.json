{
  "name": "osh-web.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "grommet-icons": "^0.10.1",
    "postcss-custom-properties": "^6.1.0",
    "postcss-import": "^10.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hyperscript": "^3.0.0",
    "react-markdown": "^2.5.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "toml-loader": "^1.0.0"
  },
  "homepage": "http://osh-web.github.io",
  "scripts": {
    "start": "react-static start",
    "build": "react-static build",
    "serve": "serve dist -p 3000",
    "push": "gh-pages -d dist -b master",
    "clean": "rm -rf dist",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "deploy": "npm run build && npm run push"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "babel": {},
  "eslintConfig": {
    "extends": "react-app"
  },
  "main": "index.js",
  "repository": "https://github.com/osh-web/osh-web.github.com",
  "author": "Tomohiko Himura <eiel.hal@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-react-tools": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages": "^1.0.0",
    "react-static": "^1.0.40",
    "rebass": "^1.0.4",
    "serve": "^6.2.0"
  }
}
